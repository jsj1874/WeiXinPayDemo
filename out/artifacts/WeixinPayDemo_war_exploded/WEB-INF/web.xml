<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
    <display-name>WeixinPayDemo</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>pcPay</servlet-name>
        <servlet-class>com.wy.servlet.PcPayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pcPay</servlet-name>
        <url-pattern>/pcPay</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoadPayImageServlet</servlet-name>
        <servlet-class>com.wy.servlet.LoadPayImageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadPayImageServlet</servlet-name>
        <url-pattern>/loadPayImage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PayNotifyUrlServlet</servlet-name>
        <servlet-class>com.wy.servlet.PayNotifyUrlServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PayNotifyUrlServlet</servlet-name>
        <url-pattern>/notifyUrl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoadPayStateServlet</servlet-name>
        <servlet-class>com.wy.servlet.LoadPayStateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadPayStateServlet</servlet-name>
        <url-pattern>/loadPayState</url-pattern>
    </servlet-mapping>
    <servlet>W
        <servlet-name>wapPay</servlet-name>
        <servlet-class>com.wy.servlet.WapPayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wapPay</servlet-name>
        <url-pattern>/wapPay</url-pattern>
    </servlet-mapping>
</web-app>